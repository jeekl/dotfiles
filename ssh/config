Host jeekl.me
        port 2222
        user jeff
        IdentityFile ~/.ssh/id_dsa

Host smartos
	user root

host ftp.klarna.com
        user ftp

host saxfisk
        user root

host jenkins-*
        user root

host rapid-depl-*
        user root

host *
	ControlPath ~/.ssh/ssh-%r@%h:%p
	Compression yes
        # Keep ssh from timing out when leaving the connection idle.
        ServerAliveInterval 300
