Compression  yes
ForwardAgent yes

host jeekl.me
     port 2222
     user jeff

host localhost
     NoHostAuthenticationForLocalhost yes

host *
     user jeekl
     # Keep ssh from timing out when leaving the connection idle.
     ServerAliveInterval 300
     ControlMaster auto
     ControlPath ~/.ssh/sockets/%r@%h:%p
     ControlPersist yes
